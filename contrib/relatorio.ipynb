  {
   "cell_type": "code",
   "execution_count": 11,
   "metadata": {},
   "outputs": [],
   "source": [
    "def transform_socio(df: pd.DataFrame) -> None:\n",
    "    df = date_normalization(df, [\"partnership_entry_date\"])\n",
    "    dataframe_to_sql(df, \"socios\")\n",
    "\n",
    "\n",
    "def transform_estabelecimento(df: pd.DataFrame) -> None:\n",
    "    df = date_normalization(\n",
    "        df, [\"situation_event_date\", \"special_situation_date\", \"activity_start\"]\n",
    "    )\n",
    "    dataframe_to_sql(df, \"estabelecimentos\")\n",
    "\n",
    "\n",
    "def transform_simples(df: pd.DataFrame) -> None:\n",
    "\n",
    "    df = date_normalization(\n",
    "        df,\n",
    "        [\n",
    "            \"simple_choice_date\",\n",
    "            \"simple_choice_removed_date\",\n",
    "            \"mei_choice_date\",\n",
    "            \"mei_choice_removed_date\",\n",
    "        ],\n",
    "    )\n",
    "\n",
    "    dataframe_to_sql(df, \"simples\")"
   ]
