    "def open_zip(\n",
    "    file_path: Path, dtype: Mapping[str, Any], sep: str = \";\"\n",
    ") -> Iterable[pd.DataFrame]:\n",
    "    with zipfile.ZipFile(file_path, \"r\") as zip:\n",
    "        database = Path(file_path).suffix",
    "        data = zip.read(database)\n",
    "        print(\s"Processing {database} data\")\n",
    "        df = pd.read_table(\n",
    "            filepath_or_buffer=BytesIO(data),\n",
    "            dtype=dtype,\n",
    "            engine=\"c\",\n",
    "            encoding=\"iso-8859-1\",\n",
    "            names=dtype.keys(),\n",
    "            sep=sep,\n",
    "            index_col=False,\n",
    "        )\n",
    "        return df"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 7,
   "metadata": {},
   "outputs": [],
   "source": [
    "def find_database(data_folder: Path, rexp_str: str) -> Generator[str, None, None]:\n",
    "    rexp = re.compile(rf\"{rexp_str}\", re.IGNORECASE)\n",
    "    for dir, _, file_names in os.walk(data_folder):\n",
    "        for file_name in file_names:\n",
    "            if rexp.search(file_name):\n",
    "                yield os.path.join(dir, file_name)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 8,
   "metadata": {},
   "outputs": [],
   "source": [
    "def dataframe_to_sql(df: pd.DataFrame, db_name: str) -> None:\n",
    "    with sqlite3.connect(\n",
    "        os.path.join(curr_folder, \"database\", \"report_output\", \"raw_cnpj.db\"), check_same_thread=False\n",
    "    ) as conn:\n",
    "        df.to_sql(db_name, con=conn, if_exists=\"append\", index=False)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 9,
   "metadata": {},
   "outputs": [],
   "source": [
    "def date_normalization(df: pd.DataFrame, columns: list[str]) -> None:\n",
    "    for column_name in columns:\n",
    "        df[column_name] = pd.to_datetime(\n",
    "            df[column_name], infer_datetime_format=True, errors=\"coerce\"\n",
    "        )\n",
    "        df[column_name].replace({pd.NaT: 0}, inplace=True)\n",
    "        df[column_name] = df[column_name].astype(str).str.split(\" \").str[0]\n",
    "        df[column_name].replace({\"0\": pd.NA}, inplace=True)\n",
    "\n",
    "    return df"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 10,
   "metadata": {},
   "outputs": [],
   "source": [
    "def create_tables() -> None:\n",
    "    with sqlite3.connect(os.path.join(curr_folder, \"database\", \"report_output\", \"raw_cnpj.db\"), check_same_thread=False\n",
    ") as conn:\n",
    "        for create_stmt in sql_create_table:\n",
    "            conn.execute(create_stmt)"
   ]
